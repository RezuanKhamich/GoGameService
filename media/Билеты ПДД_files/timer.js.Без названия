$(document).ready(function() {
  var time = new Date(0);
  var formatedTime = new String();

  var timerId = setInterval(function () {
    time = new Date(time.getTime() + 1000);
    formatedTime = time.getMinutes() + ':' + time.getSeconds();
    $('#timer').text(formatedTime);
  }, 1000);

  setTimeout(function() {
    clearInterval(timerId);

    $.ajax({
      type: 'POST',
      url: 'api/ajax-quiz.php',
      response:'html',
      data: {
        autolose: 1
      },
      success: function(data) {
        $('#clickToLose').trigger('click');
      }
    });
  }, 1200000);
});
